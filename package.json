    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^7.10.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "rimraf": "^2.6.3",
    "typeorm": "^0.2.25",
    "uuid": "^3.3.2",
    "voca": "^1.4.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "winston-elasticsearch": "^0.7.9"
